public class Banks{
    public static void main(String[] args) {
        Bank bank [] = new Bank[3];
        bank [0] = new PrivatBank();
        bank [1] = new AlphaBank();
        bank [2] = new NaftogazBank();
        for(int i = 0;i < bank.length; i++){
            bank[i].deposit(180);
            bank[i].withDraw(800);
            bank[i].deposit(220);
            System.out.println(bank[i].account());
        }      

    }
